{
  "name": "qwik-lib",
  "version": "0.0.1",
  "description": "Create a reusable Qwik component library",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.16.2",
    "@builder.io/qwik-city": "0.1.0-beta9",
    "@storybook/addon-essentials": "7.0.0-beta.28",
    "@storybook/addon-interactions": "7.0.0-beta.28",
    "@storybook/addon-links": "7.0.0-beta.28",
    "@storybook/blocks": "7.0.0-beta.28",
    "@storybook/html": "7.0.0-beta.28",
    "@storybook/html-vite": "7.0.0-beta.28",
    "@storybook/testing-library": "^0.0.13",
    "@types/eslint": "8.4.10",
    "@types/node": "^18.11.16",
    "@types/node-fetch": "2.6.2",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint": "8.32.0",
    "eslint-plugin-qwik": "0.16.2",
    "eslint-plugin-storybook": "^0.6.10",
    "node-fetch": "3.3.0",
    "np": "7.6.3",
    "prettier": "2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.0.0-beta.28",
    "storybook-framework-qwik": "*",
    "typescript": "4.9.4",
    "undici": "5.15.0",
    "vite": "4.0.4"
  }
}
