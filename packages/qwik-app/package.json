{
  "name": "qwik-app",
  "version": "0.0.0",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=22.0.0"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode dev",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "storybook": "storybook dev -p 6006",
    "storybook.build": "storybook build",
    "test-storybook": "test-storybook"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.16.1",
    "@builder.io/qwik-city": "1.16.1",
    "@storybook/addon-a11y": "10.1.0",
    "@storybook/addon-docs": "10.1.0",
    "@storybook/builder-vite": "10.1.0",
    "@storybook/test-runner": "0.24.1",
    "@types/eslint": "9.6.1",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "eslint": "8.57.1",
    "eslint-plugin-qwik": "1.16.1",
    "node-fetch": "3.3.2",
    "playwright": "1.55.1",
    "prettier": "3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "10.1.0",
    "storybook-framework-qwik": "workspace:*",
    "typescript": "5.9.2",
    "undici": "7.16.0",
    "vite": "7.1.7",
    "vite-tsconfig-paths": "5.1.4"
  }
}
